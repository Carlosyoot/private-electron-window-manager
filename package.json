    "LICENCE",
    "README.md"
  ],

  
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "electron": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "electron": "^25.0.0",
    "typescript": "^5.0.0"
  },

  "keywords": [
    "electron",
    "window-manager",
    "ipc",
    "typescript",
    "builder",
    "router"
  ],
